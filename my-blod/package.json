    "test": "node scripts/test.js --env=jsdom",
    "server": "cross-env NODE_ENV=development && tsc -p tsconfig.server.json && nodemon build/dist/server/server.js",
    "debug": "nodemon --watch ./build --inspect=0.0.0.0:5858 --nolazy ./build/dist/server/server.js"
